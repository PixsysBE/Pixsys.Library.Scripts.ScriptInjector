<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Pixsys.Library.Scripts.ScriptInjector</id>
    <title>Pixsys.Library.Scripts.ScriptInjector</title>
    <description>This middleware and IResultFilter will inject scripts or script paths to your HTML in script tags containing a nonce</description>
    <authors>Sylvain Signore</authors>
    <version>0.0.0.0</version>
    <readme>README.md</readme>
    <projectUrl>https://github.com/PixsysBE/Pixsys.Library.Scripts.ScriptInjector</projectUrl>
        <dependencies>
      <group targetFramework="net8.0" >
        <dependency id="NetEscapades.AspNetCore.SecurityHeaders" version="0.24.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\**\*.dll" target="lib" />
    <file src="bin\Release\**\*.xml" target="lib" />
    <file src="..\..\..\README.md" target="README.md" />
  </files>
</package>